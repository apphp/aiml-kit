{
    "name": "apphp/ml-kit",
    "description": "PHP-ML - SDK designed to bring AI and Machine Learning capabilities to PHP.",
    "version": "0.0.1",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Samuel Akopyan",
            "email": "leumas.a@gmail.com"
        }
    ],
    "require": {
        "php": "~8.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Apphp\\MLKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apphp\\MLKit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "phpunit --colors=always --configuration phpunit.xml --testsuite all",
        "tests:coverage": "phpunit --colors=always --configuration phpunit.xml --testsuite all --coverage-html coverage",
        "test": "phpunit --colors=always --configuration phpunit.xml --testsuite all --filter"
    }
}
